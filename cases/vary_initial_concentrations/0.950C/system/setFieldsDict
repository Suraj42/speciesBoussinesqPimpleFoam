FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue C       0
);

regions
(
    boxToCell
    {
        box    (0.09585 0 0) (0.10715 0.0113 0.00015); 

        fieldValues
        (
            volScalarFieldValue C 1
        );
    }
);


